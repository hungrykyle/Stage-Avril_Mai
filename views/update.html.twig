{% extends "layout.html.twig" %}


{% block title %}{% endblock %}


{% block content %}
       <div class="starter-template">
       <h1>{{keyword.Keyword}}</h1>
         <div class="form-group">
        {{ form_start(form_modif,{ 'attr':  {
            'class': 'form-inline',}}) }}
           {{ form_errors(form_modif.keyword) }}
            {{ form_widget(form_modif.keyword, { 'attr':  {
              'rows': '4',
            'class': 'form-control',
            }}) }}
        

<input type="submit" class="btn btn-primary" value="Modifier" />
</div>
</div>
{{ form_end(form_modif) }}

{% endblock %}